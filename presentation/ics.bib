@article{MaStGrInSp08,
 abstract = {In this paper we present a theoretical and experimental result on the control of multi-agent non-holonomic systems. We design and implement a novel decentralized control scheme that achieves dynamic formation control and collision avoidance for a group of non-holonomic robots. First, we derive a feedback law using Lyapunov-type analysis that guarantees collision avoidance and tracking of a reference trajectory for a single robot. Then we extend this result to the case of multiple non-holonomic robots, and show how different multi-agent problems, such as formation control and leader---follower control, can be addressed in this framework. Finally, we combine the above results to address the problem of coordinated tracking for a group of agents. We give extensive experimental results that validate the effectiveness of our results in all three cases.},
 author = {Mastellone, Silvia and Stipanovic, Dusan M. and Graunke, Christopher R. and Intlekofer, Koji A. and Spong, Mark W.},
 year = {2008},
 title = {Formation Control and Collision Avoidance for Multi-agent Non-holonomic Systems: Theory and Experiments},
 keywords = {Collision avoidance},
 pages = {107--126},
 volume = {27},
 number = {1},
 journal = {International Journal of Robotics Research},
 doi = {10.1177/0278364907084441}
}


@article{MaVe09,
 abstract = {We consider the problem of designing distributed controllers for a class of systems which can be obtained from the interconnection of a number of identical subsystems. If the state space matrices of these systems satisfy a certain structural property, then it is possible to derive a procedure for designing a distributed controller which has the same interconnection pattern as the plant. This procedure is basically a multiobjective optimization under linear matrix inequality constraints, with system norms as performance indices. The explicit expressions for computing these controllers are given for both {\textless}i{\textgreater}H{\textless}/i{\textgreater} {\textless}sub{\textgreater}infin{\textless}/sub{\textgreater} or {\textless}i{\textgreater}H{\textless}/i{\textgreater} {\textless}sub{\textgreater}2{\textless}/sub{\textgreater} performance, and both for static state feedback and dynamic output feedback (in discrete time). At the end of the paper, two application examples illustrate the effectiveness of the approach.},
 author = {Massioni, Paolo and Verhaegen, Michel},
 year = {2009},
 title = {Distributed Control for Identical Dynamically Coupled Systems: A Decomposition Approach},
 keywords = {decomposition approach;Distributed control;dynamic output feedback;dynamically coupled systems;H{\textless}sub{\textgreater}2{\textless};H{\textless}sub{\textgreater}infin{\textless};interconnected systems;linear matrix inequalities;linear matrix inequality;Linear systems;multiobjective optimization;state feedback;state space matrices;state-space methods;static state feedback;sub{\textgreater} performance},
 pages = {124--135},
 volume = {54},
 number = {1},
 issn = {0018-9286},
 journal = {IEEE Transactions on Automatic Control},
 doi = {10.1109/TAC.2008.2009574}
}


@inproceedings{MaVe10,
 abstract = {This paper concerns the problem of distributed controller synthesis for a class of systems which we call ``decomposable systems''. These systems have a special property that makes it possible to decompose them into a set of modal systems and approach the distributed control problem in an easy and effective way. In a recent paper of ours, we have shown a suboptimal H{\textless}sub{\textgreater}$\infty${\textless}/sub{\textgreater} controller synthesis method based on the use of the multi-objective optimization technique making use of Linear Matrix Inequalities (LMIs); in this article we show that another suboptimal approach is possible, which exploits robust control techniques, namely the full block S-procedure. In the end we also briefly show a method based on small gain H{\textless}sub{\textgreater}$\infty${\textless}/sub{\textgreater} robust performance synthesis, and the results of the three different methods are compared on a couple of test cases.},
 author = {Massioni, Paolo and Verhaegen, Michel},
 title = {A full block S-procedure application to distributed control},
 keywords = {control system synthesis;control theory;decomposable system;Distributed control;full block S-procedure;H{\textless}sub{\textgreater}$\infty${\textless};H{\textless}sup{\textgreater}$\infty${\textless};linear matrix inequalities;linear matrix inequality;LMI;multi-objective optimization;optimisation;Optimization methods;Performance gain;robust control;robustness;state feedback;sub{\textgreater} controller;sup{\textgreater} control;Testing},
 booktitle = {American Control Conference},
 year = {2010}
}


