@article{BoVa82,
 abstract = {Each of several agents updates his estimate of the same random variable whenever he makes a new observation or receives the estimate made by another agent. In turn, each agent transmits his estimate to a randomly chosen subset of the other agents. A subset of agents forms a communicating ring if for every pair{\textless}tex{\textgreater}underline{m}, underline{p}{\textless}/tex{\textgreater}of ring members, there is a sequence of ring members{\textless}tex{\textgreater}underline{m} = underline{m}{\_}{1}, underline{m}{\_}{2}, ... , underline{m}{\_}{n+1} = underline{p}{\textless}/tex{\textgreater}such that{\textless}tex{\textgreater}underline{m}{\_}{i}{\textless}/tex{\textgreater}sends his estimate to{\textless}tex{\textgreater}underline{m}{\_}{i+1}{\textless}/tex{\textgreater}infinitely often. If each ring member knows that he is a ring member, then the estimates of all the ring members asymptotically agree. However, this common limit can depend upon the order in which estimates are transmitted.},
 author = {Borkar, V. and Varaiya, P. P.},
