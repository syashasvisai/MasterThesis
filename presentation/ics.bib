@article{Ba02,
 abstract = {Conditions for robust stability of linear time-invariant systems subject to structured linear time-invariant uncertainties can be derived in the complex {\&}mu; framework, or, equivalently, in the framework of integral quadratic constraints. These conditions can be checked numerically with linear matrix inequality (LMI)-based convex optimization using the Kalman-Yakubovich-Popov lemma. We show how LMI tests also yield a convex parametrization of (a subset of) Lyapunov functionals that prove robust stability of such uncertain systems. We show that for uncertainties that are pure delays, the Lyapunov functionals reduce to the standard Lyapunov-Krasovksii functionals that are encountered in the stability analysis of delay systems. We demonstrate the practical utility of the Lyapunov functional parametrization by deriving bounds for a number of measures of robust performance (beyond the usual H{\textless}sub{\textgreater}{\&}infin;{\textless}/sub{\textgreater} performance); these bounds can be efficiently computed using convex optimization over linear matrix inequalities.},
 author = {Balakrishnan, V.},
 year = {2002},
 title = {Lyapunov functionals in complex {\&}mu; analysis},
 keywords = {analysis;complex {\&}mu;Constraint theory;control system analysis;Feedback;functional equations;Integral equations;integral quadratic constraints;Kalman-Yakubovich-Popov lemma;linear matrix inequalities;linear matrix inequality-based convex optimization;Linear systems;linear time-invariant systems;Lyapunov functional parametrization;Lyapunov functionals;Lyapunov method;Lyapunov methods;matrix algebra;optimisation;Performance evaluation;robust control;robust performance;Robust stability;structured linear time-invariant uncertainties;Symmetric matrices;Transfer functions;Uncertain systems;Upper bound},
 pages = {1466--1479},
