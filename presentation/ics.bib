@inproceedings{YaLiChPe06,
 abstract = {In this paper, a distributed robust control scheme is proposed for formation stabilization of noholonomic wheeled mobile robots (WMRs) with parametric uncertainty and actuator saturations. A quasi-LPV model is first established to describe the error dynamics of each WMR. For a given cost function, we generate distributed optimal control problems for each interacting robot whose error dynamics and constraints are uncoupled. In the framework of LMI optimization, a distributed moving horizon H{\textless}sub{\textgreater}infin{\textless}/sub{\textgreater} control scheme is consequently formulated as on-line solving each constrained optimal problem synchronously at each sampling time. We also discuss the closed-loop properties inclusive of stability and H{\textless}sub{\textgreater}infin{\textless}/sub{\textgreater} performance. Simulation is performed to highlight the effectiveness of the proposed distributed control law},
 author = {Yang, Tiantian and Liu, Zhiyuan and Che, Hong and Pei, Run},
 title = {Distributed robust control of multiple mobile robots formations via moving horizon strategy},
 keywords = {actuator saturations;Actuators;closed loop systems;closed-loop property;Constraint optimization;Constraint theory;Cost function;cost optimal control;Distributed control;distributed control law;distributed optimal control;distributed robust control;error analysis;Error correction;error dynamics;formation stabilization;H{\textless}sub{\textgreater}infin{\textless};H{\textless}sup{\textgreater}infin{\textless};linear matrix inequalities;LMI optimization;mobile robots;moving horizon strategy;multiple mobile robots formations;multi-robot systems;nonholonomic wheeled mobile robots;Optimal control;parametric uncertainty;quasiLPV model;robust control;Sampling methods;stability;sub{\textgreater} control;sup{\textgreater} control;Uncertain systems;Uncertainty},
 booktitle = {American Control Conference},
 year = {2006},
 doi = {10.1109/ACC.2006.1656654}
}


