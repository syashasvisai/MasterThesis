 author = {Lichtenberg, Gerwald and Lunze, J.},
 title = {Proze{\ss}{\"u}berwachung mit Hilfe qualitativer Modelle},
 pages = {44--66},
 booktitle = {Fuzzy Duisburg},
 year = {1996}
}


@article{LiLu97,
 abstract = {This paper is concerned with the problem of reconstructing the qualitative values of the system states from quantised  measurements of input and output sequences by means of a qualitative model. It is assumed that the state, input and output spaces  are partitioned into disjoint regions. The numbers of the regions are used as qualitative values of the corresponding signals. The proposed observer uses a qualitative model of the continuous--variable discrete--time system. The model has the form of a nondeterministic automaton. A necessary and sufficient condition is given under which the observer generates a set of qualitative states that includes the qualitative state of the real system. The benefits of the observer for process supervision is shown by applying it to a tank system.},
 author = {Lichtenberg, Gerwald and Lunze, J.},
 year = {1997},
 title = {Observation of qualitative states by means of a qualitative model},
