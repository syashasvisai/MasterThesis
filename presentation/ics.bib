@inproceedings{SuArPa11,
 abstract = {We consider interconnections of output strictly passive systems and study their robustness in the presence of delay. We establish a new integral quadratic constraint (IQC), which captures the magnitude roll-off at high frequencies. The benefit of the roll-off IQC is exemplified in a study of the stability of a cyclic interconnection structure. When used in conjunction with the IQC describing passivity, we obtain a stability bound on the gain that depends on the size of the delay and relaxes a small-gain bound.},
 author = {Summers, E. and Arcak, Murat and Packard, A.},
 title = {Evaluating the delay robustness of interconnected passive systems with a frequency-dependent integral quadratic constraint},
 keywords = {cyclic interconnection structure;Delay;Delay effects;delay robustness;delays;frequency-dependent integral quadratic constraint;interconnected passive system;interconnected systems;Numerical stability;Power system stability;robust control;robustness;roll-off IQC;Stability criteria},
 pages = {4237--4242},
 booktitle = {American Control Conference},
 year = {2011}
}


@article{SuArPa13,
 abstract = {We consider networks of passive systems with time delays in the interconnections, and present a stability analysis technique with the help of the integral quadratic constraint (IQC) framework. Unlike the classical passivity approach that fails to characterize delay robustness, and the small-gain approach that conservatively accounts for arbitrarily large delays, the new technique gives sharp stability estimates that depend on the duration of delay. Since the effect of delay depends on its duration relative to the time scales of the system, we make use of a ``roll-off'' IQC that captures magnitude roll-off at high frequencies, thus, providing the critical time-scale information. We then combine this roll-off IQC with an output strict passivity IQC that incorporates gain and phase information, and demonstrate the benefit of this combined IQC approach on a cyclic interconnection structure with delay. Finally, we develop a technique to verify these IQCs for classes of nonlinear state-space models and present an example from Internet congestion control.},
 author = {Summers, E. and Arcak, Murat and Packard, A.},
 year = {2013},
 title = {Delay Robustness of Interconnected Passive Systems: An Integral Quadratic Constraint Approach},
 keywords = {critical time-scale information;cyclic interconnection structure;Delay;delay robustness;Delay systems;delays;Feedback loop;integral quadratic constraint approach;interconnected passive systems;interconnected systems;Internet;Internet congestion control;IQC framework;large delays;magnitude roll-off;nonlinear state-space models;nonlinear systems;output strict passivity IQC;Power system stability;robust control;roll-off IQC;sharp stability;small-gain approach;stability analysis;stability analysis technique;Stability criteria;state-space methods;telecommunication congestion control;Terrorism;time delays;Vectors},
 pages = {712--724},
 volume = {58},
 number = {3},
 issn = {0018-9286},
 journal = {IEEE Transactions on Automatic Control},
 doi = {10.1109/TAC.2012.2219972}
}


