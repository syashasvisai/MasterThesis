 keywords = {communication channel;Communication channels;communication delay;Communication system control;communication topology;control nonlinearities;control systems;Couplings;Delay effects;delays;linear coupling;multi-agent systems;Multiagent systems;nonlinear control systems;nonlinear coupling;nonlinear systems;output synchronization;Particle scattering;passive nonlinear controls;positive definite storage function;scattering variable formulation;sector nonlinearity;synchronisation;time delay;Topology},
