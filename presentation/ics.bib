@inproceedings{GhSh10,
 abstract = {In this paper, we consider the problem of designing distributed controller for networks of dynamic systems called decomposable systems with switching network topologies. Exploiting certain structural property associated with state space matrices of these systems, a modal decomposition approach is used to design a distributed controller that has the same interconnection structure as the networks in each instance; i.e. controller structure changes according to network topologies without any delay. An explicit expression for computing the controller parameters is derived using LMI approach under H{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} or H{\textless}sub{\textgreater}$\infty${\textless}/sub{\textgreater} criteria with D-stability property. Moreover, a simple sufficient condition for the existence of such a controller is provided that ensures the stability of the network with any arbitrary switching provided that the networks switch among a finite set of admissible topologies. The simulations results are provided to support the theoretical development.},
 author = {Ghadami, R. and Shafai, B.},
 title = {Robust distributed control of decomposable systems with switching network topology},
 keywords = {control system synthesis;decomposable systems;Distributed control;D-stability property;dynamic network systems;linear matrix inequalities;LMI approach;Matrix decomposition;modal decomposition approach;multi-agent systems;Network topology;Output feedback;robust control;robust distributed controller design;stability;state space matrices;Switches;switching network topology;switching networks;Symmetric matrices;Topology},
 booktitle = {49th IEEE Conference on Decision and Control},
 year = {2010},
 doi = {10.1109/CDC.2010.5718137}
}


@article{GhSh13,
 abstract = {In this technical note, we consider the problem of designing distributed controller for a continuous-time system composed of a number of identical dynamically coupled subsystems. The underlying mathematical derivation is based on Kronecker product and a special similarity transformation constructed from interconnection pattern matrix that decomposes the system into modal subsystems. This along with the result of extended linear matrix inequality (LMI) formulation for continuous-time systems makes it possible to derive explicit expressions for computing the parameters of distributed controllers for both static state feedback and dynamic output feedback cases. The main contribution of the technical note is the solution of distributed control problem for continuous-time systems under {\textless}i{\textgreater}H{\textless}/i{\textgreater}{\textless}sub{\textgreater}$\infty${\textless}/sub{\textgreater}/ \textgreek{a}-stability and {\textless}i{\textgreater}H{\textless}/i{\textgreater}{\textless}sub{\textgreater}2{\textless}/sub{\textgreater}/ \textgreek{a}-stability performances by solving a set of LMIs with noncommon Lyapunov variables. The effectiveness of this method is demonstrated by means of a satellite formation example.},
 author = {Ghadami, R. and Shafai, B.},
 year = {2013},
 title = {Decomposition-Based Distributed Control for Continuous-Time Multi-Agent Systems},
 keywords = {artificial satellites;continuous time systems;continuous-time multiagent systems;control system synthesis;decomposition-based distributed control;Distributed control;distributed controller design;dynamic output feedback;H{\textless}sub{\textgreater}2{\textless};H{\textless}sub{\textgreater}$\infty${\textless};H{\textless}sup{\textgreater}2{\textless};H{\textless}sup{\textgreater}$\infty${\textless};identical dynamically coupled subsystem;interconnection pattern matrix;Kronecker product;linear matrix inequalities;linear matrix inequality;linear matrix inequality (LMI);LMI;Lyapunov methods;mathematical derivation;modal subsystem;multi-agent systems;noncommon Lyapunov variables;Output feedback;performance index;robust control;satellite formation;Satellites;similarity transformation;stability;Stability criteria;Standards;state feedback;static state feedback;sub{\textgreater};sup{\textgreater} control;system decomposition;Upper bound;\textgreek{a}-stability performance},
 pages = {258--264},
