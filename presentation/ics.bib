@inproceedings{LiLu95b,
 abstract = {This paper concerns the design of qualitative controllers, which have access only to a global information about the current state  of the plant. It will be shown that such controllers can be systematically designed by means of a qualitative model of the plant and, moreover, that the closed loop can be analysed by means of such a model. A performance index is defined for evaluating the behaviour of the closed--loop system and  the design problem  formulated as a discrete optimisation problem, which is solved by a branch--and--bound algorithm. The results are illustrated by experimental data obtained for the ''inverted pendulum''.},
 author = {Lichtenberg, Gerwald and Lunze, J.},
 title = {Systematic design of rule-based controllers by means of a qualitative model of the plant},
 pages = {853--857},
 booktitle = {The European Congress on Intelligent Techniques and Soft Computing},
 year = {1995}
}


